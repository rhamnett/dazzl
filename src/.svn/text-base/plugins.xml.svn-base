<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<bean id="asterisk" class="org.hamnett.adm.asterisk.impl.AsteriskPlugin"/>

	<bean id="audio" class="org.hamnett.adm.audio.impl.AudioPlugin">
		<property name="asterisk"><ref local="asterisk" /></property>
	</bean>

	<bean id="cisco" class="org.hamnett.adm.cisco.impl.CiscoPlugin">
		<property name="asterisk"><ref local="asterisk" /></property>
	</bean>

	<bean id="bluetooth" class="org.hamnett.adm.bluetooth.impl.BluetoothPlugin">
		<property name="asterisk"><ref local="asterisk" /></property>
	</bean>

	<bean id="notification" class="org.hamnett.adm.notification.impl.NotificationPopupPlugin">
		<property name="asterisk"><ref local="asterisk" /></property>
        <property name="cisco"><ref local="cisco" /></property>
    </bean>

	<bean id="phonebook" class="org.hamnett.adm.phonebook.impl.PhoneBookPlugin">
		<property name="asterisk"><ref local="asterisk" /></property>
    </bean>

    <bean id="crm" class="org.hamnett.adm.crm.impl.CrmPlugin">
		<property name="asterisk"><ref local="asterisk" /></property>
    </bean>
    
	<bean id="adinsight" class="org.hamnett.adm.adinsight.impl.AdInsightPlugin">
		<property name="asterisk"><ref local="asterisk" /></property>
    </bean>

</beans>