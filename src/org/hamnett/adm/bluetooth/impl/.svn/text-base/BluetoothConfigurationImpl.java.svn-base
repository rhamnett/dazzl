package org.hamnett.adm.bluetooth.impl;

import org.hamnett.adm.bluetooth.BluetoothConfiguration;

/**
 * Default implementation of BluetoothConfiguration.
 * 
 * @author srt
 * @version $Id: BluetoothConfigurationImpl.java,v 1.2 2005-08-13 00:19:28 srt Exp $
 */
public class BluetoothConfigurationImpl implements BluetoothConfiguration
{
    private boolean enabled;
    private String deviceAddress;
    private String redirectExtension;

    public boolean isEnabled()
    {
        return enabled;
    }

    public void setEnabled(boolean enabled)
    {
        this.enabled = enabled;
    }

    public String getDeviceAddress()
    {
        return deviceAddress;
    }

    public void setDeviceAddress(String deviceAddress)
    {
        this.deviceAddress = deviceAddress;
    }

    public String getRedirectExtension()
    {
        return redirectExtension;
    }

    public void setRedirectExtension(String redirectExtension)
    {
        this.redirectExtension = redirectExtension;
    }
}
